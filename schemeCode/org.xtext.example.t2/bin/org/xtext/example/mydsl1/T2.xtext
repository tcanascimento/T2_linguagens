grammar org.xtext.example.mydsl1.T2 with org.eclipse.xtext.common.Terminals

generate t2 "http://www.xtext.org/example/mydsl1/T2"

Model:
	vars+=VAR*;
	
Op:
	'-' | '+' | '/' | '*' | '<' | '>'  | '>=' | '<=' | '='
;

Define:
	'(''define' ID Complemento')'
;

Complemento:
	INT | '(' Op INT*')' | '(' Op ID INT')' 
;

Value: 
	Define | Complemento
;

VAR:
	Value 
;
